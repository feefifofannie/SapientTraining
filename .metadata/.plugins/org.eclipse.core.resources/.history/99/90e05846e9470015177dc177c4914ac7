package prac;

// Compute the area of a circle.
/**
 * 
 * @author dredmo
 *
 */
class Area {
	public static void main(String[] args) {
		final double PI = 3.1416;
		final double RADIUS = 10.8;
		double area;

		area = PI * RADIUS * RADIUS; // compute area

		System.out.println("Area of circle is " + area);
	}
}