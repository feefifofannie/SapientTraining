/**
 * 
 */
package staticexample;

class Data {
	static int value;
}
/**
 * @author dredmo
 *
 */
public class StaticMain {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Data data1 = new Data();
		Data data2 = new Data();
		
		data1.value = 4;
		data2.value = 8;
		
		System.out.println(data1.value+"    "+data2.value);

	}

}
