package prac;

// Compute the area of a circle.
class Area {
	public static void main(String[] args) {
		double pi;
		double radius;
		double a;

		radius = 10.8; // radius of circle
		pi = 3.1416; // pi, approximately
		a = pi * radius * radius; // compute area

		System.out.println("Area of circle is " + a);
	}
}